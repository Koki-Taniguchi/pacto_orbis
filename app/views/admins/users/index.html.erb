<div class="container">
<h1 class="m-5">ユーザー一覧</h1>
<table class="table">
  <tr><th>ユーザーネーム</th><th>ステータス</th><th></th></tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.first_name %> <%= user.last_name %></td>
    <td><%= user.status %></td>
    <td><%= link_to '詳細', admins_user_path(user) %> <%= link_to '削除', admins_user_path(user), method: :delete %></td>
  </tr>
  <% end %>
</table>
<%= paginate @users %>
</div>